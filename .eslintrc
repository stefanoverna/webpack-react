{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-no-bind": 0,
    "space-before-function-paren": [ 2, "never" ],
    "jsx-a11y/href-no-hash": 0,
    "react/prefer-stateless-function": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}

